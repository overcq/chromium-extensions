{ "manifest_version": 3
, "minimum_chrome_version": "114"
, "default_locale": "en"
, "name": "Site Comment ©overcq"
, "short_name": "site‐comment"
, "version": "0.0.0.270"
, "description": "Show global comments for a site."
, "action":
  { "default_title": "Open side panel with the site comments"
  }
, "side_panel":
  { "default_path": "side_panel.html"
  }
, "background":
  { "service_worker": "service_worker.js"
  }
, "permissions":
  [ "sidePanel"
  , "storage"
  , "tabs"
  ]
, "externally_connectable":
  { "ids": []
  }
}
