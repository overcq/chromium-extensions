{ "manifest_version": 3
, "minimum_chrome_version": "99"
, "default_locale": "en"
, "author":
  { "email": "overcq@int.pl"
  }
, "name": "page image ©overcq"
, "short_name": "page-image"
, "version": "0.6.0.116"
, "description": "Adds ‛create page image’."
, "icons":
  { "48": "logo_48.png"
  }
, "background":
